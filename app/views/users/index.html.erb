<header class="page-header">
  <h1>
    <%= fa('user') %> Users
    <small>
      <%= link_to(fa('plus') + ' New User', new_user_path) %>
    </small>
  </h1>
</header>


<% if @users.present? %>

  <ol>
    <% @users.each do |user| %>
      <li>
        <%= link_to(user.username, user_path(user)) %>
      </li>
    <% end %>
  </ol>

<% else %>

  <p class="text-danger">Why for are there no more users no more?</p>

<% end %>

